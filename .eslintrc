{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "extends": ["airbnb", "plugin:react/recommended", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "React": true,
    "document": true,
    "window": true,
    "localStorage": true,
    "fetch": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": ["js", "jsx"]
      }
    ],
    "prettier/prettier": ["error"]
  }
}
